<h2 class="champion-stats" style="margin-top:40px">最常见的符文</h2>
<div class="rune-collection">
  <% for(var i = 0; i < championData.runes.mostGames.runes.length; i++){ %>
  <div class="rune-type-area" style="width:98%">
    <div class="rune-img">
      <img src="//ddragon.leagueoflegends.com/cdn/<%= core.ddPatch %>/img/rune/<%= championData.runes.mostGames.runes[i].img %>" /> <strong><%= championData.runes.mostGames.runes[i].number %>x</strong> <span class="rune-title"><%= championData.runes.mostGames.runes[i].name %> </span>
      <span class="rune-benefits"><small><%= championData.runes.mostGames.runes[i].description %></small></span>
    </div>
    <div class="rune-info">
    </div>
  </div>
  <% } %>
</div>
<div class="build-text">
  <strong><%= championData.runes.mostGames.winPercent %>%</strong> 胜率 | <strong><%= championData.runes.mostGames.games %></strong> 局比赛
</div>


